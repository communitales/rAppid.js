<ui:AutoCompleteBoxClass xmlns:ui="js.ui" xmlns="http://www.w3.org/1999/xhtml" xmlns:js="js.core">
    <input type="{type}" value="{{search}}" cid="input" placeholder="{placeholder}" class="inputClass" />
    <ui:ItemsView items="{proposals}" tagName="ul">
        <js:Template name="item">
            <ui:TemplatePlaceHolder templateName="proposal" />
        </js:Template>
    </ui:ItemsView>

    <js:Template name="proposal">
        <li>{item}</li>
    </js:Template>

</ui:AutoCompleteBoxClass>