<?xml version="1.0"?>
<app:GridClass xmlns="http://www.w3.org/1999/xhtml"
               xmlns:app="app" xmlns:js="js.core" xmlns:ui="js.ui" xmlns:conf="js.conf">

    <js:Imports>
        <!--
            additional required classes, not references in this dependency tree
        -->
    </js:Imports>

    <Router>
        <Route route="asdasd" exec="" />
    </Router>

    <js:ModuleLoader>
        <conf:Module route="articles.*" module="app.modules.Articles"/>
        <conf:Module route="designs.*" module="app.modules.Designs"/>

        <header>
            <js:ContentPlaceHolder name="header" />
        </header>
        <div id="wrapper">
            <js:ContentPlaceHolder name="main"/>
        </div>
    </js:ModuleLoader>

</app:GridClass>
